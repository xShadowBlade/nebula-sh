{
  "name": "nebula-sh",
  "version": "0.1.0",
  "homepage": "https://github.com/xShadowBlade/nebula-sh#readme",
  "bugs": {
    "url": "https://github.com/xShadowBlade/nebula-sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xShadowBlade/template-defaults.git"
  },
  "license": "MIT",
  "author": "xShadowBlade",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node-repl": {
      "types": "./dist/types/indexNode.d.ts",
      "default": "./dist/indexNode.js"
    }
  },
  "main": "./test/run.js",
  "scripts": {
    "build": "esbuild ./src/index.ts ./src/indexNode.ts --bundle --minify --platform=node --sourcemap --outdir=./dist/ --format=esm",
    "dev": "esbuild ./src/index.ts ./src/indexNode.ts --bundle --platform=node --sourcemap --outdir=./dist/ --watch --format=esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir ./dist/types",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lz-string": "^1.5.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "peerDependencies": {
    "@xterm/xterm": "^5.5.0"
  }
}
